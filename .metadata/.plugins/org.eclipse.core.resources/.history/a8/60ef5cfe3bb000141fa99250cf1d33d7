package com.Atieh.crm_mobile_webService;

import java.util.HashMap;
import java.util.Map;

public class authenticationJSONClass {

	private String Salt;
	private com.Atieh.crm_mobile_webService.Status Status;
	private String Token;

	/**
	 * 
	 * @return
	 * The Salt
	 */
	public String getSalt() {
		return Salt;
	}

	/**
	 * 
	 * @param Salt
	 * The Salt
	 */
	public void setSalt(String Salt) {
		this.Salt = Salt;
	}

	/**
	 * 
	 * @return
	 * The Status
	 */
	public com.Atieh.crm_mobile_webService.Status getStatus() {
		return Status;
	}

	/**
	 * 
	 * @param Status
	 * The Status
	 */
	public void setStatus(com.Atieh.crm_mobile_webService.Status Status) {
		this.Status = Status;
	}

	/**
	 * 
	 * @return
	 * The Token
	 */
	public String getToken() {
		return Token;
	}

	/**
	 * 
	 * @param Token
	 * The Token
	 */
	public void setToken(String Token) {
		this.Token = Token;
	}

}